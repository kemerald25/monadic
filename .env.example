# Monadic Meetups Environment Variables
# Copy this file to .env and fill in your actual values

# Reown (Web3Modal) Configuration
VITE_REOWN_PROJECT_ID=your_reown_project_id_here

# Blockchain Configuration
VITE_MONAD_RPC_URL=https://testnet-rpc.monad.xyz
VITE_MONAD_EXPLORER_URL=https://testnet-explorer.monad.xyz
VITE_CHAIN_ID=41454

# Smart Contract Addresses (fill after deployment)
VITE_EVENT_FACTORY_ADDRESS=
VITE_TICKET_NFT_ADDRESS=
VITE_SPONSORSHIP_NFT_ADDRESS=

# The Graph Configuration
VITE_SUBGRAPH_URL=https://api.thegraph.com/subgraphs/name/your-username/monadic-meetups
VITE_GRAPH_API_KEY=your_graph_api_key_here

# Backend Services
VITE_RELAYER_URL=https://relayer.monadic-meetups.com
VITE_ANALYTICS_API_URL=https://api.monadic-meetups.com
VITE_WEBSOCKET_URL=wss://ws.monadic-meetups.com

# IPFS Configuration
VITE_IPFS_GATEWAY=https://gateway.pinata.cloud/ipfs/
VITE_PINATA_API_KEY=your_pinata_api_key
VITE_PINATA_SECRET_KEY=your_pinata_secret_key

# External APIs
VITE_GOOGLE_MAPS_API_KEY=your_google_maps_api_key
VITE_MAPBOX_ACCESS_TOKEN=your_mapbox_token

# Analytics & Monitoring
VITE_SENTRY_DSN=your_sentry_dsn
VITE_MIXPANEL_TOKEN=your_mixpanel_token

# Development
VITE_ENABLE_DEVTOOLS=true
VITE_LOG_LEVEL=debug

# Production flags
VITE_ENVIRONMENT=development